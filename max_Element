import java.util.*;
public class Main
{
    static Node root;
    static class Node{
        int data;
        Node left;
        Node right;
        Node(int d){
            data = d;
            left = right = null;
        }
    }
    static int max_Element(Node temp){
        int max = Integer.MIN_VALUE;
        if(temp==null)
            return max;
        Queue<Node> q = new LinkedList<Node>();
        q.add(temp);
        while(!q.isEmpty()){
            temp = q.poll();
            if(temp.data>max)
                max = temp.data;
            if(temp.left!=null)
                q.add(temp.left);
            if(temp.right!=null)
                q.add(temp.right);
        }
        return max;
    }
	public static void main(String[] args){
		root = new Node(6);
		root.left = new Node(4);
		root.right = new Node(8);
		root.left.left = new Node(3);
		root.left.right = new Node(5);
		root.right.left = new Node(7);
		root.right.right = new Node(10);
		System.out.print("Maximum Element : " + max_Element(root));
		
	}
}
